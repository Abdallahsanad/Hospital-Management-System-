@model ConsultantViewModel

<form asp-action="Delete">
    <div class="form-group">
        <label asp-for="Name" class="col-form-label"></label>
        <input readonly asp-for="Name" class="form-control" />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Address" class="col-form-label"></label>
        <input readonly asp-for="Address" class="form-control" />
        <span asp-validation-for="Address" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Salary" class="col-form-label"></label>
        <input readonly asp-for="Salary" class="form-control" />
        <span asp-validation-for="Salary" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Email" class="col-form-label"></label>
        <input readonly asp-for="Email" class="form-control" />
        <span asp-validation-for="Email" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Phone" class="col-form-label"></label>
        <input readonly asp-for="Phone" class="form-control" />
        <span asp-validation-for="Phone" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="DateOfBirth" class="col-form-label"></label>
        <input readonly asp-for="DateOfBirth" class="form-control" />
    </div>
    <input hidden asp-for="ImageName" />

    <input type="hidden" asp-for="Id" /> <input type="submit" value="Delete" class="btn btn-danger" />
    <a asp-controller="Consultant" asp-action="Index" class="btn btn-light">Back To List</a>
</form>
